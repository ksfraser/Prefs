{
  "name": "ksfraser/prefs",
  "version": "0.1.0",
  "description": "Cross-platform preferences abstraction (DB table, WordPress, SuiteCRM, FrontAccounting)",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "KS Fraser"
    }
  ],
  "require": {
    "php": ">=7.4",
    "ksfraser/ksf-modules-dao": "^0.1"
  },
  "repositories": [
    {
      "type": "path",
      "url": "../ksf_ModulesDAO"
    }
  ],
  "autoload": {
    "psr-4": {
      "Ksfraser\\Prefs\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Ksfraser\\Prefs\\Tests\\": "tests/"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^9.0"
  },
  "scripts": {
    "test": "@php vendor/bin/phpunit"
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}
